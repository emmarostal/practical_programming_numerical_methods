set terminal svg
set terminal svg background "white"
set output "qsine.svg"
set key top right
set xlabel "x"
set xzeroaxis
set xrange [0:5]
set ylabel "y"
set tics out
set grid
set title "Quadratic interpolation of sin(x)"
plot sin(x), "qinterp.txt" using ($1):($12) with point pt 7 ps 0.9 title "Values from list"\
, "qinterp.txt" using ($5):($13) with point pt 7 ps 0.9 lc rgb "web-blue" title "Interpolated values"\
, "qinterp.txt" using ($5):($14) with lines lc rgb "black" title "Integral" 

